<h1><%= @place["name"] %></h1>

<h2 class = "text-success">My Entries</h2>
<div class="container">
  <div class="row">
    <% @entries.each do |entry| %>
      <div class="col-md-4">
        <h3><%= entry["title"] %></h3>
        <h4><%= entry["occurred_on"] %></h4>
        <p><%= entry["description"] %></p>
      </div>
    <% end %>
  </div>
</div>


<p>
  <a class = "btn btn-primary" href="/entries/new?place_id=<%= @place["id"] %>">New Entry</a>
</p>

<p>
  <a class = "btn btn-primary" href="/places">Back to All Places</a>
</p>